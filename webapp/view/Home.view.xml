<mvc:View controllerName="sap.ui.demo.MyApp.controller.Home" displayBlock="true" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc">
	<Page id="page" title="{i18n>homePageTitle}">
		<content>
			<Button text="{i18n>homeNavBtnTitle}" press="onPressNavBtn"></Button>
			<List items="{ODataNorthwindModel>/Products}">
				<items>
					<ObjectListItem title="{ODataNorthwindModel>ProductName}"></ObjectListItem>
				</items>
			</List>

		</content>
	</Page>
</mvc:View>